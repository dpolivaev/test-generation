            name="Templates">
            <keywordReference id="org.dpolivaev.dsl.tsgen.ui.keyword_StrategyDsl"/>
        </page>
    </extension>
    <extension
            point="org.eclipse.ui.propertyPages">
        <page
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
