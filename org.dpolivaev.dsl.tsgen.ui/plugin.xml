<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

    <extension
            point="org.eclipse.ui.editors">
        <editor
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="tsgen"
            id="org.dpolivaev.dsl.tsgen.StrategyDsl"
            matchingStrategy="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.xbase.ui.editor.JavaEditorInputMatcher"
            name="StrategyDsl Editor">
        </editor>
    </extension>
    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="org.dpolivaev.dsl.tsgen.StrategyDsl.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
            commandId="org.dpolivaev.dsl.tsgen.StrategyDsl.validate">
         <activeWhen>
            <reference
                    definitionId="org.dpolivaev.dsl.tsgen.StrategyDsl.Editor.opened">
            </reference>
         </activeWhen>
      	</handler>
      	<!-- copy qualified name -->
        <handler
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
				<reference definitionId="org.dpolivaev.dsl.tsgen.StrategyDsl.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
            	<and>
            		<reference definitionId="org.dpolivaev.dsl.tsgen.StrategyDsl.XtextEditor.opened" />
	                <iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="org.dpolivaev.dsl.tsgen.StrategyDsl.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="org.dpolivaev.dsl.tsgen.StrategyDsl" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
        <definition id="org.dpolivaev.dsl.tsgen.StrategyDsl.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
                        value="org.dpolivaev.dsl.tsgen.StrategyDsl" 
                        forcePluginActivation="true"/>
                </with>        
            </and>
        </definition>
    </extension>
    <extension
            point="org.eclipse.ui.preferencePages">
        <page
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="org.dpolivaev.dsl.tsgen.StrategyDsl"
            name="StrategyDsl">
            <keywordReference id="org.dpolivaev.dsl.tsgen.ui.keyword_StrategyDsl"/>
        </page>
        <page
            category="org.dpolivaev.dsl.tsgen.StrategyDsl"
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
