            label="Build Tasks"
            modes="run, debug">
          <contextualLaunch>
           <enablement>
             <with variable="selection">
               <count value="1"/>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt type="org.eclipse.core.resources.IFile"/>
                    <test property="org.eclipse.debug.ui.matchesPattern"
                          value="*.tsgen"/>
               </iterate>
             </with>
           </enablement>
           <contextLabel
              mode="run"
              label="Generation Task"/>
          <contextLabel
              mode="debug"
              label="Generation Task"/>
         </contextualLaunch>
      </shortcut>
   </extension>
</plugin>
