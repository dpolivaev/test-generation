            name="Syntax Coloring">
            <keywordReference id="org.dpolivaev.dsl.tsgen.ui.keyword_StrategyDsl"/>
        </page>
        <page
            category="org.dpolivaev.dsl.tsgen.StrategyDsl"
            class="org.dpolivaev.dsl.tsgen.ui.StrategyDslExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
